{
  "name": "minecraft-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "cypress:open": "cypress open",
    "lint": "vue-cli-service lint",
    "cytest": "npm run serve & wait-on http://localhost:8080"
  },
  "dependencies": {
    "3": "^2.1.0",
    "-": "^0.0.1",
    "@microsoft/signalr": "^3.1.8",
    "@popperjs/core": "^2.5.4",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.19.0",
    "core-js": "^3.6.5",
    "cypress": "6.0.0",
    "jquery": "1.9.1",
    "oidc-client": "^1.10.1",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "sweetalert2": "^10.11.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.6.0",
    "wait-on": "^5.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-unit-jest": "^4.5.8",
    "@vue/cli-service": "^4.5.0",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "start-server-and-test": "^1.11.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-components": "warn",
      "vue/no-unused-vars": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
